      <basicHttpBinding>
        <binding name="BasicHttpBindingImagen" maxBufferPoolSize="524288" maxBufferSize="524288" maxReceivedMessageSize="524288"/>
      </basicHttpBinding>
    </bindings>
    <services>
      <service behaviorConfiguration="SampleServiceBehavior" name="InstitutoKhipuERP.SL.Servicio">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="BasicHttpBindingImagen"
          contract="InstitutoKhipuERP.SL.IServicioInstitutoKhipu" />
